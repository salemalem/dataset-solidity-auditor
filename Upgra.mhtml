        <span class=3D"n">owner</span> <span class=3D"o">=3D</span> <span c=
lass=3D"nb">msg</span><span class=3D"p">.</span><span class=3D"n">sender</s=
pan><span class=3D"p">;</span>
    <span class=3D"p">}</span>

