n> <span class=3D"o">=3D=3D</span> <span class=3D"n">owner</span><span clas=
s=3D"p">)</span>
        <span class=3D"kr">_</span><span class=3D"p">;</span>
    <span class=3D"p">}</span>

    <span class=3D"kd">function</span> <span class=3D"n">changeBackend</spa=
n><span class=3D"p">(</span><span class=3D"kt">address</span> <span class=
=3D"n">newBackend</span><span class=3D"p">)</span> <span class=3D"kr">publi=
c</span>
    <span class=3D"n">onlyOwner</span><span class=3D"p">()</span>
