frame src=3D"cid:frame-21A54B489DF85DF2360E063A75683E75@mhtml.blink" allowf=
ullscreen=3D"" frameborder=3D"0" height=3D"192" width=3D"680" title=3D"Shad=
owingVariable.sol" class=3D"eh n ei ug bg" scrolling=3D"no"></iframe></div>=
