<span class=3D"k">contract</span><span class=3D"w"> </span><span class=3D"n=
i">Fund</span><span class=3D"w"> </span><span class=3D"p">{</span>
<span class=3D"w">    </span><span class=3D"c1">/// @dev Mapping of ether s=
hares of the contract.</span>
<span class=3D"w">    </span><span class=3D"kt">mapping</span><span class=
=3D"p">(</span><span class=3D"kt">address</span><span class=3D"w"> </span><=
span class=3D"o">=3D&gt;</span><span class=3D"w"> </span><span class=3D"kt"=
>uint</span><span class=3D"p">)</span><span class=3D"w"> </span>shares<span=
 class=3D"p">;</span>
