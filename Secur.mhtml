<span class=3D"w">            </span>shares<span class=3D"p">[</span><span =
class=3D"k">msg.sender</span><span class=3D"p">]</span><span class=3D"w"> <=
/span><span class=3D"o">=3D</span><span class=3D"w"> </span><span class=3D"=
m m-Decimal">0</span><span class=3D"p">;</span>
